<digester-rules>
    <pattern value="aspect-system">
        <object-create-rule classname="com.tirsen.nanning.config.AspectSystem" />
    </pattern>

    <pattern value="aspect-system/class">
        <factory-create-rule classname="com.tirsen.nanning.xml.ClassAspectFactory" />
        <set-next-rule methodname="addAspect" />
    </pattern>

    <pattern value="*/aspect">
        <object-create-rule attrname="class" />
        <set-next-rule methodname="addAspect" />
    </pattern>

    <pattern value="*/interceptor">
        <factory-create-rule classname="com.tirsen.nanning.xml.InterceptorAspectFactory" />
        <set-next-rule methodname="addAspect" />
    </pattern>

    <pattern value="*/mixin">
        <factory-create-rule classname="com.tirsen.nanning.xml.IntroductorAspectFactory" />
        <set-next-rule methodname="addAspect" />
    </pattern>

    <pattern value="*/pointcut">
        <factory-create-rule classname="com.tirsen.nanning.xml.PointcutFactory" />
        <set-next-rule methodname="setPointcut" />
    </pattern>

</digester-rules>